{
  "name": "3d-viewer-crud",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "tailwind": "npx tailwindcss -i src/index.css -o public/css/index.css --watch",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "mix": "mix watch",
    "watch": "concurrently \"npm run start\" \"npm run mix\"",
    "dev": "cross-env NODE_ENV=development webpack  --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hmr": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.28.1",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gulp": "^5.0.0",
    "laravel-mix": "^6.0.49",
    "tailwindcss": "^3.4.17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
